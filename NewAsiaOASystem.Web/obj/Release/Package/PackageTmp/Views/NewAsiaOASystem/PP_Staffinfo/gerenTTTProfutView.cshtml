@{
    ViewBag.Title = "个人团体收入支持项目查看";
    Layout = "~/Views/Shared/_SysLayout.cshtml";
}

<style>
    .table_con
    {
        border-collapse: collapse;
        border: none;
    }

        .table_con td
        {
            border: solid #eae5e5 1px;
            height: 35px;
            padding: 0 0 0 5px;
            font-size:12px;
        }

    .btnspan button
    {
        width: 120px;
        height: 30px;
        background: #337CA8;
    }
</style>

<script type="text/javascript">
    $(function () {
        ajaxTTshourudata('@ViewData["stafId"]');
        ajaxTTzhichudata('@ViewData["stafId"]');
    })
</script>

<div id="ProManage" class="easyui-tabs" style="width: 90%; height: 320px; margin: 20px; padding: 0px;">
    <div id="TTshouruButtonAuth" title="团体收入项" style="overflow: auto; padding: 20px;">
    </div>
    <div id="TTzhichuButtonAuth" title="团体支出项" style="overflow: auto; padding: 20px;">
    </div>
</div>
<div style="text-align: center;width:100%;">
    <a href="javascript:void(0)" class="easyui-linkbutton" onclick="AddTTProshouru('@ViewData["stafId"]')">团体收入项增加</a>
    <a href="javascript:void(0)" class="easyui-linkbutton" onclick="AddTTProzhichu('@ViewData["stafId"]')">团体支出项增加</a>
</div>
<script src="~/Scripts/NAjs/PP_pulice.js"></script>

<div id="windowDia" class="easyui-window" closed="true" style="width: 100%; height: 400px; padding: 5px;"></div>
