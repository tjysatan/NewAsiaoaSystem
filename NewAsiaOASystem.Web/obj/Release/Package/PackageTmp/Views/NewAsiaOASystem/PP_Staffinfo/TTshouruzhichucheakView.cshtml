@{
    ViewBag.Title = "团队收支项（当月）查看";
    Layout = "~/Views/Shared/_SysLayout.cshtml";
}

<style>
    .table_con
    {
        border-collapse: collapse;
        border: none;
    }

        .table_con td
        {
            border: solid #eae5e5 1px;
            height: 35px;
            padding: 0 0 0 5px;
            font-size: 12px;
        }

    .btnspan button
    {
        width: 120px;
        height: 30px;
        background: #337CA8;
    }
</style>

<script type="text/javascript">
    $(function () {
        ajaxTTszjsonbyteamId('@ViewData["TeamId"]', "0");
        ajaxTTszjsonbyteamId('@ViewData["TeamId"]', "1");
    })
</script>

<div>

    <table class="table_con" style="width: 90%; margin-left: 25px;">
        <tr>
            <td style="font-size: 12px;">当月团队项收支明细</td>
            <td>当前团队：@ViewData["TeamName"] </td>
           @* <td>当月支出：@ViewData["zhichu"] 元</td>*@
           @*  <td id="yingli"></td>*@
        </tr>
    </table>
</div>

<div id="ProManage" class="easyui-tabs" style="width: 90%; height: 320px; margin: 20px; padding: 0px;">
    <div id="TTmxshouruButtonAuth" title="团体收入明细" style="overflow: auto; padding: 20px;">
    </div>
    <div id="TTmxzhichuButtonAuth" title="团体支出明细" style="overflow: auto; padding: 20px;">
    </div>
</div>
<div style="text-align: center; width: 100%;">
    <a href="javascript:void(0)" class="easyui-linkbutton" onclick="AddTTshourumx('@ViewData["teamId"]')">团体收入项增加</a>
    <a href="javascript:void(0)" class="easyui-linkbutton" onclick="AddTTzhichumx('@ViewData["teamId"]')">团体支出项增加</a>
</div>
<script src="~/Scripts/NAjs/PP_pulice.js"></script>

<div id="windowDia" class="easyui-window" closed="true" style="width: 100%; height: 400px; padding: 5px;"></div>
