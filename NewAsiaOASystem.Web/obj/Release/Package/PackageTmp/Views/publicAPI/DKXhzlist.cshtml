                    }
                    if(jsonStr[i].Ysdatetime!=""&&jsonStr[i].Ysdatetime!=null)
                    {
                        html+='<td width="5%">'+showDate(jsonStr[i].Ysdatetime)+'---'+tab(showDate(jsonStr[i].YJJQtime),showDate(jsonStr[i].Ysdatetime))+'</td>';
                    }
                    else
                    {
                        html+='<td width="5%">'+showDate(jsonStr[i].YJJQtime)+'/-</td>';
                    }
                    html+='<td width="10%"><div class="layui-btn-group">'+listbtnshow(jsonStr[i].Id)+'</div></td>';
                    html+='</tr>';
