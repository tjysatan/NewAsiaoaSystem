        //订单客服提交
        function KFTJAJAX(Id)
        {
            $.ajax({
                url: "../DKX_DDtypeinfo/kfDDtjEide",
                type: "POST",
                data: { Id: Id},
                dataType: "html",
                async: true,
                beforeSend: function () {
                    ddchen();
                },
                success: function (response) {
                    disLoading();
                    if (response == "0") {
                        layer.alert("订单不存在！", { icon: 2 }, function () { location.reload(); });
                    }
                    if (response == "1") {
                        layer.alert("订单已经提交,无需再次提交！", { icon: 2 }, function () { location.reload(); });
                    }
                    if (response == "2")
                    {
                        layer.alert("该订单尚未关联需求/料单,没法提交！", { icon: 2 }, function () { location.reload(); });
                    }
                    if (response == "3")
                    {
                        layer.alert("提交异常,请重新提交！", { icon: 2 }, function () { location.reload(); });
                    }
                    if(response=="4")
                    {
                        layer.alert("提交成功！", { icon: 1 }, function () { location.reload(); });
                    }
                },
                error: function (e) {
                    disLoading();
                    layer.alert("操作失败！", { icon: 2 });
                }
            })
        }

