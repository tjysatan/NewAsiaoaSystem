@{
    ViewBag.Title = "占比修改";
    Layout = "~/Views/Shared/_SysLayout.cshtml";
}

<style type="text/css">
    .input_textcss
    {
        border: 1px solid #bbb3b3;
        height: 30px;
        width: 90%;
    }
</style>

<input type="hidden" id="stafId" name="stafId" value='@ViewData["stafId"]' />
<input type="hidden" id="Id" name="Id" value='@ViewData["Id"]' />
<div class="h-table">
    
    <div class="h-table-content">
        <div id="content">
            <table class="table_listNew">

                <tr>
                    <td colspan="2" style="border: none"></td>
                </tr>
                <tr>
                    <td>当前占比：</td>
                    <td>@ViewData["zhanbi"]%</td>
                </tr>
                <tr>
                    <td width="30%">占比：</td>
                    <td align="left">
                        <input type="text" id="zhanbi" name="zhanbi" class="input_textcss" placeholder="输入占比率" />%</td>
                </tr>

            </table>
        </div>
        <div style="text-align: center; width: 100%;">
            <a href="javascript:void(0)" class="easyui-linkbutton" onclick="gerenTTzhanbiEide()">提交,关闭页面</a>
           
        </div>
    </div>
</div>

<script src="~/Scripts/NAjs/PP_pulice.js"></script>