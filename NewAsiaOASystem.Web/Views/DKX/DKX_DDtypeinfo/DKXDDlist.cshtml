        //订单删除
        function dddetAjax(val,type)
        {
            if(type=="-1"||type=="1")
            {
                $.messager.confirm("操作提示", '确定要删除订单吗？', function (data) {
                    if(data)
                    {
                        $.ajax({
                            url: "../DKX_DDtypeinfo/DDdelEide",
                            type: "POST",
                            data: { Id: val},
                            dataType: "html",
                            async: true,
                            beforeSend: function () {
                                ddchen();
                            },
                            success: function (response) {
                                disLoading();
                                if (response == "0") {
                                    layer.alert("删除成功！", { icon: 1 }, function () { location.reload(); });
                                }
                                if (response == "4") {
                                    layer.alert("删除失败！", { icon: 2 });
                                }
                                if (response == "3")
                                {
                                    layer.alert("当前状态不可以删除！", { icon: 2 });
                                }
                                if (response == "2")
                                {
                                    layer.alert("订单不存在！", { icon: 2 });
                                }
                                if(response=="1")
                                {
                                    layer.alert("删除异常！", { icon: 2 });
                                }
                            },
                            error: function (e) {
                                disLoading();
                                layer.alert("操作失败！", { icon: 2 });
                            }
                        })
                    }
                })
            }
            else
            {
                layer.alert("当前状态不可以删除！", { icon: 2 }, function () { location.reload(); });
            }
        }

