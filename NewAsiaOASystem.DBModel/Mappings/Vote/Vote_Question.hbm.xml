    <property name="S_Id">
      <column name="S_Id"/>
    </property>

    <property name="T_Id">
      <column name="T_Id"/>
    </property>

    <property name="Q_Count">
      <column name="Q_Count"/>
    </property>

    <property name="Q_time">
      <column name="Q_time"/>
    </property>

    <property name="Q_Person">
      <column name="Q_Person"/>
